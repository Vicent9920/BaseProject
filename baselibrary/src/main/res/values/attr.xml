<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CircleDrawable">
        <attr name="circle_radius" format="reference|float"/>
        <attr name="circle_border" format="reference|float"/>
        <attr name="circle_color" format="color"/>
    </declare-styleable>

    <declare-styleable name="SwitchButton" >
        <!-- 是否选中 -->
        <attr name="android:checked" />
        <!-- 是否禁用 -->
        <attr name="android:enabled" />
    </declare-styleable>
</resources>